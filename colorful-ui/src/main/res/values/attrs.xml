<?xml version="1.0" encoding="utf-8"?>
<resources >

    <attr name="mLeftImage" format="color|reference"/>
    <attr name="mLeftImageId" format="reference|integer"/>
    <attr name="mLeftImageVisibility">
        <enum name="gone" value="8" />
        <enum name="invisible" value="4" />
        <enum name="visible" value="0" />
    </attr>

    <attr name="mLeftTitleId" format="reference|integer"/>
    <attr name="mLeftTitleText" format="string"/>
    <attr name="mLeftTitleWidth" format="dimension"/>
    <attr name="mLeftTitleTextSize" format="dimension"/>
    <attr name="mLeftTitleTextColor" format="color"/>
    <attr name="mLeftTitleTextVisibility">
        <enum name="gone" value="8" />
        <enum name="invisible" value="4" />
        <enum name="visible" value="0" />
    </attr>

    <attr name="mRightTextId" format="reference|integer"/>
    <attr name="mRightText" format="string"/>
    <attr name="mRightTextSize" format="dimension"/>
    <attr name="mRightTextColor" format="color"/>
    <attr name="mRightTextVisibility">
        <enum name="gone" value="8" />
        <enum name="invisible" value="4" />
        <enum name="visible" value="0" />
    </attr>

    <attr name="mRightImage" format="color|reference"/>
    <attr name="mRightImageId" format="reference|integer"/>
    <attr name="mRightImageVisibility">
        <enum name="gone" value="8" />
        <enum name="invisible" value="4" />
        <enum name="visible" value="0" />
    </attr>

    <declare-styleable name="DateSelectView">
        <attr name="mBackground" format="color|reference"/>

        <attr name="mLeftViewId" format="reference|integer"/>
        <attr name="mLeftViewSrc" format="color|reference"/>
        <attr name="mLeftViewVisibility">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>

        <attr name="mTitleViewId" format="reference|integer"/>
        <attr name="mTitleViewText" format="string"/>
        <attr name="mTitleViewTextSize" format="dimension"/>
        <attr name="mTitleViewTextColor" format="color"/>
        <attr name="mTitleViewTextVisibility">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>

        <attr name="mRightViewId" format="reference|integer"/>
        <attr name="mRightViewSrc" format="color|reference"/>
        <attr name="mRightViewVisibility">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>

        <attr name="mTodayViewId" format="reference|integer"/>
        <attr name="mTodayViewSrc" format="color|reference"/>
        <attr name="mTodayViewVisibility">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>
    </declare-styleable>

    <!-- ****************标题栏*******************-->
    <declare-styleable name="TitleBar">
        <attr name="mLeftDrawable" format="color|reference"/>
        <attr name="mLeftImageId"/>
        <attr name="mLeftImageVisibility"/>

        <attr name="mTitleText" format="string"/>
        <attr name="mTitleTextSize" format="dimension"/>
        <attr name="mTitleColor" format="color"/>
        <attr name="mTitleTextVisibility">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>

        <attr name="mRightTextId"/>
        <attr name="mRightText"/>
        <attr name="mRightTextSize"/>
        <attr name="mRightTextColor"/>
        <attr name="mRightTextVisibility"/>

        <attr name="mRightDrawable" format="color|reference"/>
        <attr name="mRightImageId"/>
        <attr name="mRightImageVisibility"/>
    </declare-styleable>

    <!-- **************** ItemView *******************-->
    <declare-styleable name="ItemView">
        <attr name="mLeftImage" />
        <attr name="mLeftImageId"/>
        <attr name="mLeftImageVisibility"/>

        <attr name="mLeftTitleId"/>
        <attr name="mLeftTitleText"/>
        <attr name="mLeftTitleWidth"/>
        <attr name="mLeftTitleTextSize"/>
        <attr name="mLeftTitleTextColor"/>
        <attr name="mLeftTitleTextVisibility"/>

        <attr name="mRightTextId" />
        <attr name="mRightText"/>
        <attr name="mRightTextSize"/>
        <attr name="mRightTextColor"/>
        <attr name="mRightTextVisibility"/>

        <attr name="mRightImage"/>
        <attr name="mRightImageId"/>
        <attr name="mRightImageVisibility"/>

        <attr name="mBottomLineColor" format="color"/>
    </declare-styleable>


</resources >
